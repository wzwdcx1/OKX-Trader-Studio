<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
                    xmlns:wpf="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf">
    <!--功能字体、提示字体、图标、分隔线、排料结果控件的颜色-->
    <SolidColorBrush x:Key="FunctionFontColor">#FF232222</SolidColorBrush>
    <SolidColorBrush x:Key="PromptFontColor">#FF959393</SolidColorBrush>
    <!--<SolidColorBrush x:Key="IconColor">#FF375F83</SolidColorBrush>-->
    <SolidColorBrush x:Key="IconColor">#FF232222</SolidColorBrush>
    <SolidColorBrush x:Key="IconColorReserve">#FF2A2A2A</SolidColorBrush>
    <SolidColorBrush x:Key="SeparatorColor">#FFDBD9D8</SolidColorBrush>
    <SolidColorBrush x:Key="LayoutandLogsColor">#FFA79FB5</SolidColorBrush>
    <!--工具栏背景色-->
    <SolidColorBrush x:Key="ToolBarColor">#FFEEF0F0</SolidColorBrush>
    <!--左侧栏背景色-->
    <SolidColorBrush x:Key="ParameterListColor">#FFB9B6BD</SolidColorBrush>
    <!--空区背景色-->
    <SolidColorBrush x:Key="FreeAreaColor">#FFE8E8EA</SolidColorBrush>
    <!--菜单按钮高亮背景色-->
    <SolidColorBrush x:Key="MenuHighLightColor">#FFBEBEBF</SolidColorBrush>
    <!--功能图标big-->
    <Style TargetType="materialDesign:PackIcon" x:Key="FunctionIcon">
        <Setter Property="Width" Value="40"></Setter>
        <Setter Property="Height" Value="40"></Setter>
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="Foreground" Value="{DynamicResource FontBrush}"></Setter>
    </Style>
    <!--动画图标mid-->
    <Style TargetType="materialDesign:PackIcon" x:Key="AnimationIcon">
        <Setter Property="Width" Value="38"></Setter>
        <Setter Property="Height" Value="38"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="Foreground" Value="{DynamicResource FontBrush}"></Setter>
    </Style>
    <!--工艺图标small-->
    <Style TargetType="materialDesign:PackIcon" x:Key="TechIcon">
        <Setter Property="Width" Value="18"></Setter>
        <Setter Property="Height" Value="18"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="Foreground" Value="{DynamicResource FontBrush}"></Setter>
    </Style>
    <!--创建标准零件图标-->
    <Style TargetType="materialDesign:PackIcon" x:Key="CreatePartsIcon">
        <Setter Property="Width" Value="35"></Setter>
        <Setter Property="Height" Value="35"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        <Setter Property="Foreground" Value="{DynamicResource FontBrush}"></Setter>
        <Setter Property="Margin" Value="0"></Setter>
    </Style>
    <!--创建标准零件开孔图标-->
    <Style TargetType="materialDesign:PackIcon" x:Key="CreatePartsHoleIcon">
        <Setter Property="Width" Value="25"></Setter>
        <Setter Property="Height" Value="25"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        <Setter Property="Foreground" Value="{DynamicResource FontBrush}"></Setter>
        <Setter Property="Margin" Value="0"></Setter>
    </Style>
    <!--顶层工具栏使用的字体-->
    <Style TargetType="Label" x:Key="FontofToolBar">
        <Setter Property="FontSize" Value="12"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        <Setter Property="Foreground" Value="{DynamicResource FontBrush}"></Setter>
    </Style>
    <!--顶层工具栏功能按钮之间的Margin设置-->
    <Thickness x:Key="MarginofFunction">3,15,3,15</Thickness>
    <!--顶层工具栏工艺按钮之间的Margin设置-->
    <Thickness x:Key="MarginofTech">1,0,1,0</Thickness>
    <!--动画按钮之间的Margin设置-->
    <Thickness x:Key="MarginofAnimation">0,30,0,0</Thickness>
    <!--小工艺列所属的stackpanel的Margin设置-->
    <Thickness x:Key="MarginofTechStackPanel">0,10,0,0</Thickness>
    <!--工艺设置窗体中每行的Margin-->
    <Thickness x:Key="MarginofTechWindows">0 2 0 2</Thickness>
    <!--图形工艺字体的padding设置-->
    <Thickness x:Key="PaddingofTechFont">2,5,5,5</Thickness>
    <!--创建标准零件按钮的Margin设置-->
    <Thickness x:Key="MarginofCreateParts">2 4 2 2</Thickness>
    <!--图层按钮之间的Margin设置-->
    <Thickness x:Key="MarginofLayerButton">0 10 0 0</Thickness>
    <!--顶层工具栏较长的分隔线-->
    <Style TargetType="Separator" x:Key="LongSeparatorofToolBar">
        <Setter Property="Margin" Value="8,0,8,0"></Setter>
        <Setter Property="Background" Value="{DynamicResource BorderBrush}"></Setter>
        <Setter Property="LayoutTransform">
            <Setter.Value>
                <RotateTransform Angle="90"></RotateTransform>
            </Setter.Value>
        </Setter>
    </Style>
    <!--顶层工具栏较短的分隔线-->
    <Style TargetType="Separator" x:Key="ShortSeparatorofToolBar">
        <Setter Property="Margin" Value="3,12,3,5"></Setter>
        <Setter Property="Background" Value="{DynamicResource BorderBrush}"></Setter>
        <Setter Property="LayoutTransform">
            <Setter.Value>
                <RotateTransform Angle="90"></RotateTransform>
            </Setter.Value>
        </Setter>
    </Style>
    <!--工艺设置窗体样式-->
    <Style x:Key="WindowStyle" TargetType="Window">
        <Setter Property="WindowStyle" Value="None"></Setter>
        <Setter Property="AllowsTransparency" Value="True"></Setter>
    </Style>
    <!--工艺窗体大字样式-->
    <Style x:Key="LabelOfTechDecribeStyle" TargetType="Label">
        <Setter Property="FontSize" Value="16"></Setter>
        <Setter Property="FontWeight" Value="Bold"></Setter>
        <Setter Property="Foreground" Value="{DynamicResource FontBrush}"></Setter>
    </Style>
    <!--主页面工艺按钮下拉菜单-->
    <Style x:Key="TechBtnMenuItemStyle" TargetType="{x:Type MenuItem}" BasedOn="{x:Null}">
        <Setter Property="Padding" Value="-10 0 -35 0" />
        <!-- 调整MenuItem的内边距 -->
        <Setter Property="HeaderTemplate">
            <Setter.Value>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal" Margin="0">
                        <Image Source="{Binding Icon}" Margin="-1 0 0 0" HorizontalAlignment="Left"/>
                        <TextBlock Foreground="{DynamicResource FontBrush}" FontSize="12" Padding="0" FontFamily="Microsoft YaHei" Text="{Binding}" VerticalAlignment="Center" HorizontalAlignment="Left" Cursor=""/>
                    </StackPanel>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--二级菜单样式-->
    <Style x:Key="SecondTechBtnMenuItemStyle" TargetType="MenuItem">
        <Setter Property="Padding" Value="0 0 -35 0" />
        <Setter Property="Background" Value="{DynamicResource TabBrush}"/>
        <!-- 调整MenuItem的内边距 -->
        <Setter Property="HeaderTemplate">
            <Setter.Value>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal" Margin="0">
                        <Image Source="{Binding Icon}" Margin="1 0 0 0" HorizontalAlignment="Right"/>
                        <TextBlock Foreground="{DynamicResource FontBrush}" FontSize="12" Padding="0" FontFamily="Microsoft YaHei" Text="{Binding}" VerticalAlignment="Center" HorizontalAlignment="Left" Cursor=""/>
                    </StackPanel>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="OriginTypeOfGroupBox" TargetType="GroupBox"></Style>
    <!--DockPanel长度设置-->
    <Style x:Key="PatternModelDockPanel" TargetType="DockPanel">
        <Setter Property="Width" Value="270"></Setter>
    </Style>

</ResourceDictionary>